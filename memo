sqlite>  .schema food_enquetes
CREATE TABLE IF NOT EXISTS "food_enquetes" ("id" integer NOT NULL PRIMARY KEY, "name" varchar NOT NULL, "mail" varchar NOT NULL, "age" integer NOT NULL, "food_id" integer NOT NULL, "score" integer NOT NULL, "request" varchar NOT NULL, "present_id" integer NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);

CREATE TABLE               "food_enquetes" ("id" integer NOT NULL PRIMARY KEY, "name" varchar NOT NULL, "mail" varchar NOT NULL, "age" integer NOT NULL, "food_id" integer NOT NULL, "score" integer NOT NULL, "request" varchar NOT NULL, "present_id" integer NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);